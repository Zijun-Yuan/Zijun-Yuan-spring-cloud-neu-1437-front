main');\r\n          ElMessage.success('登录成功');\r\n        } else {\r\n          ElMessage.error('账号或密码不正确，请重试');\r\n          loginForm.value.password = ''; // 清空密码输入框\r\n        }\r\n      }\r\n    };\r\n\r\n    // 注册方法\r\n    const handleRegister = () => {\r\n      router.push('/supervisor/register');\r\n    };\r\n\r\n    return {\r\n      loginForm,\r\n      handleLogin,\r\n      handleRegister,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n/* 样式可以根据需要自行调整 */\r\n.login-container {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  height: 100vh;\r\n}\r\n\r\n.login-image {\r\n  max-width: 500px;\r\n  max-height: 500px;\r\n  border-radius: 50%;\r\n  /* 将左侧图片变为圆形 */\r\n  margin-right: 20px;\r\n  /* 新加入的样式，增加与表单之间的间隔 */\r\n}\r\n\r\n.right-section {\r\n  max-width: 300px;\r\n  padding: 20px;\r\n  border: 1px solid #e0e0e0;\r\n  border-radius: 5px;\r\n}\r\n\r\n.login-form {\r\n  max-width: 300px;\r\n  margin: 0 auto;\r\n}\r\n</style>\r\n"],"mappings":";AA4BA,SACEA,GAAE,QACG,KAAK;AACZ,SACEC,SAAQ,QACH,YAAY;AACnB,SACEC,kBAAiB,QACZ,0BAA0B;AACjC,SACEC,SAAQ,QACH,cAAc;AAErB,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,KAAKA,CAAA,EAAG;IACN;IACA,MAAMC,MAAK,GAAIL,SAAS,CAAC,CAAC;IAC1B;IACA,MAAMM,eAAc,GAAIL,kBAAkB,CAAC,CAAC;IAC5C;IACA,MAAMM,SAAQ,GAAIR,GAAG,CAAC;MACpBS,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE;IACZ,CAAC,CAAC;;IAEF;IACA,MAAMC,WAAU,GAAI,MAAAA,CAAA,KAAY;MAC9B,MAAMC,IAAG,GAAI;QACXH,KAAK,EAAED,SAAS,CAACK,KAAK,CAACJ,KAAK;QAC5BC,QAAQ,EAAEF,SAAS,CAACK,KAAK,CAACH;MAC5B,CAAC;MACD,IAAI,CAACF,SAAS,CAACK,KAAK,CAACJ,KAAI,IAAK,CAACD,SAAS,CAACK,KAAK,CAACH,QAAQ,EAAE;QACvDP,SAAS,CAACW,KAAK,CAAC,WAAW,CAAC;MAC9B,OAAO;QACL,MAAMP,eAAe,CAACQ,eAAe,CAACH,IAAI,CAAC;QAC3C,IAAIL,eAAe,CAACS,UAAU,CAACP,KAAI,KAAMG,IAAI,CAACH,KAAK,EAAE;UACnD,MAAMH,MAAM,CAACW,IAAI,CAAC,kBAAkB,CAAC;UACrCd,SAAS,CAACe,OAAO,CAAC,MAAM,CAAC;QAC3B,OAAO;UACLf,SAAS,CAACW,KAAK,CAAC,cAAc,CAAC;UAC/BN,SAAS,CAACK,KAAK,CAACH,QAAO,GAAI,EAAE,EAAE;QACjC;MACF;IACF,CAAC;;IAED;IACA,MAAMS,cAAa,GAAIA,CAAA,KAAM;