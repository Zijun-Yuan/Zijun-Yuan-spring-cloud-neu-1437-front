sorState.supervisor.telId === data.telId){\r\n          await router.push('/supervisor/main');\r\n          ElMessage.success('登录成功');\r\n        }else {\r\n          errorMessage.value = '账号或密码不正确，请重试';\r\n          loginForm.value.password = '';  // 清空密码输入框\r\n        }\r\n      }\r\n    };\r\n\r\n    // 注册方法\r\n    const handleRegister = () => {\r\n      router.push('/supervisor/register');\r\n    };\r\n\r\n    return {\r\n      loginForm,\r\n      handleLogin,\r\n      errorMessage,\r\n      handleRegister,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n/* 样式可以根据需要自行调整 */\r\n.login-container {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  height: 100vh;\r\n}\r\n.login-image {\r\n  max-width: 500px;\r\n  max-height: 500px;\r\n  border-radius: 50%; /* 将左侧图片变为圆形 */\r\n  margin-right: 20px; /* 新加入的样式，增加与表单之间的间隔 */\r\n}\r\n.right-section {\r\n  max-width: 300px;\r\n  padding: 20px;\r\n  border: 1px solid #e0e0e0;\r\n  border-radius: 5px;\r\n}\r\n.login-form {\r\n  max-width: 300px;\r\n  margin: 0 auto;\r\n}\r\n.login-message {\r\n  color: red;\r\n  text-align: center;\r\n  margin-top: 10px; /* 新加入的样式，增加消息与输入框之间的间隔 */\r\n}\r\n</style>"],"mappings":";AA6BA,SAAQA,GAAG,QAAO,KAAK;AACvB,SAAQC,SAAS,QAAO,YAAY;AACpC,SAAQC,kBAAkB,QAAO,0BAA0B;AAC3D,SAAQC,SAAS,QAAO,cAAc;AAEtC,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,KAAKA,CAAA,EAAG;IACN;IACA,MAAMC,MAAK,GAAIL,SAAS,CAAC,CAAC;IAC1B;IACA,MAAMM,eAAc,GAAKL,kBAAkB,CAAC,CAAC;IAC7C;IACA,MAAMM,SAAQ,GAAIR,GAAG,CAAC;MACpBS,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE;IACZ,CAAC,CAAC;IACF,MAAMC,YAAW,GAAIX,GAAG,CAAC,EAAE,CAAC;;IAE5B;IACA,MAAMY,WAAU,GAAI,MAAAA,CAAA,KAAY;MAC9B,MAAMC,IAAG,GAAI;QACXJ,KAAK,EAAED,SAAS,CAACM,KAAK,CAACL,KAAK;QAC5BC,QAAQ,EAAEF,SAAS,CAACM,KAAK,CAACJ;MAC5B,CAAC;MACD,IAAG,CAACF,SAAS,CAACM,KAAK,CAACL,KAAI,IAAK,CAACD,SAAS,CAACM,KAAK,CAACJ,QAAQ,EAAC;QACrDC,YAAY,CAACG,KAAI,GAAI,WAAW;MAClC,CAAC,MAAK;QACJ,MAAMP,eAAe,CAACQ,eAAe,CAACF,IAAI,CAAC;QAC3C,IAAIN,eAAe,CAACS,UAAU,CAACP,KAAI,KAAMI,IAAI,CAACJ,